# Production Environment Configuration
# Copy from env.example and fill with production values

# Firebase Configuration - Production Project
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Application Configuration
VITE_APP_NAME="ThisAI CRM"
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# API Configuration
VITE_API_BASE_URL=https://api.thisai-crm.com
VITE_API_TIMEOUT=30000

# External Services - Production Keys
VITE_GOOGLE_VISION_API_KEY=
VITE_OPENAI_API_KEY=
VITE_RAZORPAY_KEY_ID=

# Security Configuration - Production Settings
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_SESSION_TIMEOUT=3600000

# Feature Flags - Production Features
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_SOCIAL_LOGIN=true

# Database Configuration
VITE_FIRESTORE_CACHE_SIZE=100
VITE_INDEXEDDB_VERSION=4

# Performance Configuration - Optimized for Production
VITE_MAX_IMAGE_SIZE=5242880
VITE_IMAGE_QUALITY=0.8
VITE_CACHE_TTL=86400000




















