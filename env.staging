# Staging Environment Configuration
# Copy from env.example and fill with staging values

# Firebase Configuration - Staging Project
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Application Configuration
VITE_APP_NAME="ThisAI CRM (Staging)"
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://staging-api.thisai-crm.com
VITE_API_TIMEOUT=30000

# External Services - Staging/Test Keys
VITE_GOOGLE_VISION_API_KEY=
VITE_OPENAI_API_KEY=
VITE_RAZORPAY_KEY_ID=

# Security Configuration - Staging Settings
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
VITE_SESSION_TIMEOUT=7200000

# Feature Flags - Enable all features for testing
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_SOCIAL_LOGIN=true

# Database Configuration
VITE_FIRESTORE_CACHE_SIZE=50
VITE_INDEXEDDB_VERSION=4

# Performance Configuration - Development-like settings
VITE_MAX_IMAGE_SIZE=10485760
VITE_IMAGE_QUALITY=0.9
VITE_CACHE_TTL=3600000



















